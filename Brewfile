# Setup brew-cask
tap 'phinze/cask'
brew 'brew-cask'

# Tap useful formulae
tap 'homebrew/science'

# Install java
cask 'java'

# Install ruby formulae
brew 'rbenv'
brew 'ruby-build'

# Install python formulae
brew 'gcc'
brew 'python'
brew 'pyqt'
brew 'zmq'

# Install node formulae
brew 'npm'

# Install science formulae
brew 'beast'
brew 'beagle'
brew 'raxml'
brew 'mafft'
brew 'fasttree'
brew 'homebrew/versions/gcc49'

# Install latex formulae
cask 'mactex'
brew 'aspell', args: ['--with-lang-en']

# Install web formulae
brew 'heroku-toolbelt'

# Install AWS tools
brew 'ec2-api-tools'
